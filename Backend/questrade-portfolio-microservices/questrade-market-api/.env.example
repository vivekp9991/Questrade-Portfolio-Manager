# Server Configuration
PORT=4004
NODE_ENV=development

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/questrade_market

# Service Dependencies
AUTH_API_URL=http://localhost:4001/api
SYNC_API_URL=http://localhost:4002/api

# Market Data Configuration
ENABLE_REAL_TIME=true
QUOTE_REFRESH_INTERVAL_SECONDS=5
MARKET_DATA_CACHE_TTL=10

# Rate Limiting
QUESTRADE_RATE_LIMIT_PER_SECOND=10
MAX_SYMBOLS_PER_REQUEST=100

# Cache Configuration
ENABLE_CACHE=true
CACHE_TTL_SECONDS=10
REDIS_URL=redis://localhost:6379

# WebSocket Configuration
ENABLE_WEBSOCKET=false
WS_PORT=4005

# Logging
LOG_LEVEL=info